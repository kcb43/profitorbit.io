# Supabase Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Upstash Redis (for Vercel serverless)
UPSTASH_REDIS_REST_URL=your_upstash_redis_url_here
UPSTASH_REDIS_REST_TOKEN=your_upstash_redis_token_here

# Optional: Firebase (for push notifications)
# FIREBASE_PROJECT_ID=your_firebase_project_id
# FIREBASE_PRIVATE_KEY=your_firebase_private_key
# FIREBASE_CLIENT_EMAIL=your_firebase_client_email

# SerpAPI (used for eBay sold listings search in Add Sale)
# Get your key at https://serpapi.com/
SERPAPI_KEY=your_serpapi_key_here

# Smart Listing Features
VITE_SMART_LISTING_ENABLED=false
VITE_AI_SUGGESTIONS_ENABLED=false
VITE_DEBUG_SMART_LISTING=false
